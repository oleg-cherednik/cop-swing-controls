<?xml version="1.0" encoding="utf-8"?>
<!-- threads - maximum threads amount -->
<!-- outToConsole	- write result to console within process -->
<!-- upToDateOutput	- keep output file up to date, if 'false' - file will be saved at the end of process -->
<input threads="10" outToConsole="true" upToDateOutput="true">
	<task root="c:\" filePattern="*.txt" textPattern="java" />
	<task root="c:\" filePattern="*.txt" textPattern="info" />
</input>